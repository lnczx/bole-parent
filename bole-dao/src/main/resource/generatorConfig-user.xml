<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!--  
	  <classPathEntry location="C:\workspace\java\libs\mysql-connector-java-5.1.5-bin.jar" />
    -->
    <classPathEntry location="/workspace/work/java/libs/mysql-connector-java-5.1.33.jar" />

	<!-- 数据库链接URL、用户名、密码 -->
	<context id="jdbc-test">

		<!-- suppressAllComments 默认是false 此属性用于指定在生成的代码是否将包括任何注释。如果设置为true 则不生成注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- 数据库链接URL、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/bole_db" userId="root"
			password="root1qaz" />

		<!-- 解决数字转换问题 -->
		<javaTypeResolver>
			<!-- 只有一个属于forceBigDecimals，默认false。 如果字段精确超过0，生成BigDecimal 如果字段精确是0，总长度10-18生成Long;
				如果字段精确是0，总长5-9生成Integer; 如果字段精确是0，总长小于5生成Short; 如果forceBigDecimals为true,统一生成BigDecimal -->
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.bole.po.model.user"
			targetProject="bole-dao">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

		<!-- 生成的映射文件包名和位置 -->
		<sqlMapGenerator targetPackage="com.bole.po.xml.user"
			targetProject="bole-dao">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator targetPackage="com.bole.po.dao.user"
			targetProject="bole-dao" type="XMLMAPPER">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- 用户表 -->
		<table schema="bole_db" tableName="user"
			enableInsert="true"
			enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true"
			selectByExampleQueryId="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			enableCountByExample="false">
			<generatedKey column="user_id" sqlStatement="mysql" identity="true" />
			<columnOverride  column="user_id" property="userId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="p_id" property="pId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="add_time" property="addTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="update_time" property="updateTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			
			<columnOverride column="user_type" property="userType" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			<columnOverride column="level" property="level" javaType="java.lang.Short" jdbcType="NUMERIC"/>
		</table>
		
		<!-- 用户表升级日志表 -->
		<table schema="bole_db" tableName="user_level_log"
			enableInsert="true"
			enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true"
			selectByExampleQueryId="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			enableCountByExample="false">
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
			<columnOverride  column="id" property="id" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="user_id" property="userId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="admin_id" property="adminId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="add_time" property="addTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			
			
			<columnOverride column="log_type" property="logType" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			<columnOverride column="level_pre" property="levelPre" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			<columnOverride column="level_after" property="levelAfter" javaType="java.lang.Short" jdbcType="NUMERIC"/>

		</table>
		
		<!-- 用户团队级别统计表 -->
		<table schema="bole_db" tableName="user_level_stat"
			enableInsert="true"
			enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true"
			selectByExampleQueryId="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			enableCountByExample="false">
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
			<columnOverride  column="id" property="id" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="user_id" property="userId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="add_time" property="addTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="update_time" property="updateTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			
			<columnOverride column="level" property="level" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			
		</table>
		
		<!-- 用户提现申请表 -->
		<table schema="bole_db" tableName="user_score_cash"
			enableInsert="true"
			enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true"
			selectByExampleQueryId="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			enableCountByExample="false">
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
			<columnOverride  column="id" property="id" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="user_id" property="userId" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="add_time" property="addTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="update_time" property="updateTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			
			<columnOverride column="status" property="status" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			
		</table>
		
		
		<!-- 用户充值明细表 -->
		<table schema="bole_db" tableName="user_score_detail"
			enableInsert="true"
			enableSelectByPrimaryKey="true"
			enableUpdateByPrimaryKey="true"
			selectByExampleQueryId="false"
			enableUpdateByExample="false"
			enableDeleteByExample="false"
			enableSelectByExample="false"
			enableCountByExample="false">
			<generatedKey column="id" sqlStatement="mysql" identity="true" />
			<columnOverride  column="id" property="id" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="user_id_from" property="userIdFrom" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="user_id_to" property="userIdTo" javaType="java.lang.Long" jdbcType="NUMERIC"/>
			<columnOverride  column="add_time" property="addTime" javaType="java.lang.Long" jdbcType="NUMERIC"/>

			
			<columnOverride column="score_type" property="score_Type" javaType="java.lang.Short" jdbcType="NUMERIC"/>
			
		</table>
	


	</context>
</generatorConfiguration>